    <DataTemplate DataType="{x:Type vm:ArtistsSearchDetailViewModel}">
        <views:ArtistsSearchDetail/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PlayListsSearchDetailViewModel}">
        <views:PlayListsSearchDetail/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AlbumSearchDetailViewModel}">
        <views:AlbumSearchDetail/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SongsSearchDetailViewModel}">
