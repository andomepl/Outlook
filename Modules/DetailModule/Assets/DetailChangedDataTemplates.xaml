                    xmlns:views="clr-namespace:DetailModule.Views"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DataTemplate DataType="{x:Type vm:SearchChromeViewModel}">
        <views:SearchChromeBrowser/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:WithFilterSearchChromeViewModel}">
        <views:WithFilterSearchChrome/>
